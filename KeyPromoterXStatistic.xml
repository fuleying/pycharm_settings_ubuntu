<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Translate">
        <Item>
          <IdeaActionID>$EditorTranslateAction</IdeaActionID>
          <ShortCut>'Ctrl+Shift+Y'</ShortCut>
          <Description>Translate</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Translate and Replace...">
        <Item>
          <IdeaActionID>$TranslateAndReplaceAction</IdeaActionID>
          <ShortCut>'Ctrl+Shift+X'</ShortCut>
          <Description>Translate and Replace...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>